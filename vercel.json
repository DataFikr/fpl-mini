{
  "version": 2,
  "builds": [
    {
      "src": "fpl-league-hub/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "fpl-league-hub"
    }
  ],
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=4096",
    "NEXT_TELEMETRY_DISABLED": "1",
    "DATABASE_URL": "postgresql://placeholder:placeholder@placeholder/placeholder",
    "REDIS_URL": "redis://placeholder:6379"
  }
}